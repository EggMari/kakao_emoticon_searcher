{
  "name": "emoticon searcher",
  "description": "이모티콘 수집기능",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
        "service_worker": "background.js"
    },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "1616.png",
      "128": "128.png"
    }
  },
 "icons": {
    "16": "1616.png",
      "128": "128.png"
  }
}
